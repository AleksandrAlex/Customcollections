<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.suslovalex.customcollections">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.suslovalex.view.activity.PlayerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.suslovalex.view.activity.MainMenuActivity">

        </activity>
        <service android:name="com.suslovalex.service.ServicePlayer"
            android:exported="false"></service>
        <activity android:name="com.suslovalex.view.activity.SelectActivity"></activity>
        <provider
            android:authorities="com.suslovalex.provider"
            android:name="com.suslovalex.provider.ProviderDB"/>
    </application>

</manifest>